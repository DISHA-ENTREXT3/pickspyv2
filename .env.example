# Supabase Configuration (Get from Supabase Dashboard -> Settings -> API)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend API URL (Modal Deployment)
# You will get this URL after running 'modal deploy backend/modal_scraper.py'
VITE_BACKEND_API_URL=https://your-modal-app-name.modal.run

# Backend Secrets (For Modal)
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
FORM_SECRET=your_secure_form_secret_key

# AI Configuration (Pollinations.ai)
# Gemini 2.5 Flash Lite is the default model
POLLINATIONS_API_KEY=your_pollinations_api_key_here
AI_MODEL=gemini

# Social Media Scraping (Optional)
INSTAGRAM_USERNAME=your_ig_username
INSTAGRAM_PASSWORD=your_ig_password